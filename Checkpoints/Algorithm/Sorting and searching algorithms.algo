PROCEDURE innsertion_sort(VAR tab : ARRAY_OF INTEGER)
   VAR
     i,j,key:INTEGER;
   BEGIN
      FOR i FROM 1 TO i< tab.length STEP step DO
       key:=tab[i];
       j:=i-1;
         WHILE (j>=0 and tab[j]> key) DO
            tab[j+1]= tab[j];
            j:=j-1;  
         END_WHILE
       tab[j+1]=key;
      END_FOR
   END